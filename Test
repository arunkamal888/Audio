 @Test
    public void testTransformOutput_MeetingWithVideoFeature_VideoSDK_Offer() throws ProcessorException {
        // Arrange
        Meeting meeting = new Meeting();
        meeting.setApiVersion("v2"); // Set version to v2
        MeetingDetails details = new MeetingDetails();
        meeting.setDetails(details);

        // Create a feature with VIDEO type and VideoSDK offering
        MeetingFeature videoFeature = new MeetingFeature();
        videoFeature.setMeetingFeatureTypeName(FeatureType.VIDEO);
        Map<String, String> properties = new HashMap<>();
        properties.put(VideoSDKPropertyNameType.MEETING_OFFERING.getString(), MeetingOfferingType.VIDEOSDK.getString());
        videoFeature.setMeetingFeatureProperties(properties);

        // Add the feature to details
        details.setMeetingFeatures(Arrays.asList(videoFeature));

        // Public URL that should be set if condition is met
        String publicUrlText = "https://example.com/public-url";
